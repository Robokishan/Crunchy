pipeline {
    agent any
    stages {
        stage('Building') {
            steps {
                dir('CrunchyCrawler') {
                  sh "docker build -t robokishan/crunchy:crawler -f Dockerfile ."
                }
            }
        }
        stage('push') {
            steps {
                sh "docker image push robokishan/crunchy:crawler"
            }
        }
    }
}